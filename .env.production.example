# Production Environment Variables for Discord Chat App
# Copy this file to .env.production and fill in your actual values

# Backend API URL (FastAPI backend)
# For production: https://sahay-backend-XXX-uc.a.run.app
# For local testing: http://localhost:8000
VITE_API_URL=https://sahay-backend-XXX-uc.a.run.app

# WebSocket URL (optional - will auto-generate from VITE_API_URL if not set)
# For production: wss://sahay-backend-XXX-uc.a.run.app
# For local testing: ws://localhost:8000
VITE_WS_URL=wss://sahay-backend-XXX-uc.a.run.app
